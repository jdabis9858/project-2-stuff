</div>

<script>

    $(".sumbitted").on("submit", function(event) {
    // Make sure to preventDefault on a submit event.
    event.preventDefault();

    var newCat = {
      product_name: $("[name=searched]").val().trim()
    };

    // Send the POST request.
    $.ajax("/searched", {
      type: "POST",
      data: newCat
    }).then(
      function() {
        console.log("created new cat");
        // Reload the page to get the updated list
        location.reload();
      }
    );
  });

</script>