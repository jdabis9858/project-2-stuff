<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <ul>
        {{#each merchant1}}
        <li>
          <p>
            {{this.id}}. {{this.product_name}}

            <button data-userid="{{this.id}}" class="deluser">Delete</button>
          </p>
        </li>
        {{/each}}
